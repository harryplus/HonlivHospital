+                        // Transform Observable<Permission> to Observable<Boolean>
+                        .buffer(permissions.length)
+                        .flatMap(new Func1<List<Permission>, Observable<Boolean>>() {
+                            @Override
+                            public Observable<Boolean> call(List<Permission> permissions) {
